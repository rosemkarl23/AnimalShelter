package com.Java.Pointwest.beans;

public abstract class SoftwareTester extends Employee{
	public String firstName;
	public String lastName;
	public final static String type = "Software Tester";
	public static double salary = 850.00;
	public double hoursWorked;
	public static double tax;
	public static double nightDiff = 1.20;
	public double totalSalary;
}